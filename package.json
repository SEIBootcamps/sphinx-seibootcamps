{
  "name": "color-modes",
  "description": "Include Bootstrap's source Sass, JavaScript plugins, and color mode functionality via npm.",
  "version": "0.0.0",
  "private": true,
  "repository": "twbs/examples",
  "license": "MIT",
  "stackblitz": {
    "startCommand": "npm start"
  },
  "scripts": {
    "build": "npm run css",
    "css-compile": "sass --style compressed --source-map --embed-sources --no-error-css --load-path=node_modules scss/:src/sphinx_seibootcamps/theme/static/css/",
    "css-lint": "stylelint scss/",
    "css-prefix": "postcss --replace src/sphinx_seibootcamps/theme/static/css/styles.css --use autoprefixer --map",
    "css": "npm-run-all css-compile css-prefix",
    "css-compile-dev": "sass --style compressed --source-map --embed-sources --no-error-css --load-path=node_modules scss/:examples/kitchen-sink/_build/html/_static/css/",
    "css-prefix-dev": "postcss --replace examples/kitchen-sink/_build/html/_static/css/styles.css --use autoprefixer --map",
    "css-dev": "npm-run-all css-compile-dev css-prefix-dev",
    "server": "browser-sync start --server 'examples/kitchen-sink/_build/html' --files 'examples/kitchen-sink/_build/html/_static'",
    "start": "npm-run-all --parallel watch server",
    "watch": "nodemon -e html,scss -x \"npm run css-dev\"",
    "test": "npm-run-all css-lint css"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.7",
    "bootstrap": "^5.3.0-alpha3",
    "bootstrap-icons": "^1.10.5",
    "rfs": "^10.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "browser-sync": "^2.29.3",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "postcss-cli": "^10.1.0",
    "sass": "^1.62.1",
    "sirv-cli": "^2.0.2",
    "stylelint": "^15.6.0",
    "stylelint-config-twbs-bootstrap": "^10.0.0"
  }
}
